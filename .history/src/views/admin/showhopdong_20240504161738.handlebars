<div class="container mt-4"><table class="table">
    <a type="button" href="">Thêm hợp đồng</a>
    <thead>
      <tr>
        <th scope="col">STT</th>
        <th scope="col">Phòng</th>
        <th scope="col">Giá</th>
        <th scope="col">Ngày bắt đầu</th>
        <th scope="col">Ngày kết thúc</th>
        <th scope="col">Đặt cọc</th>
      </tr>
    </thead>
    <tbody>
      {{#each hopdong}}
        <tr>
          <th scope="row">1</th>
          <td>{{this.roomId.roomNumber}}</td>
          <td>{{this.roomPrice}}</td>
          <td>{{toLocaleString this.startDate}}</td>
          <td>{{toLocaleString this.endDate}}</td>
          <td>{{this.deposit}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>